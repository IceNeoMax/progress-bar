    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "react-scripts": "3.2.0",
    "react-toastify": "^5.4.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
